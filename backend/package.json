{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/src/server.js",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/lib-storage": "^3.985.0",
    "@azure/msal-node": "^3.6.0",
    "@js-joda/core": "^5.6.1",
    "@js-joda/locale_en": "^4.8.11",
    "@js-joda/timezone": "^2.21.1",
    "@prisma/client": "^6.13.0",
    "@prisma/extension-read-replicas": "^0.4.1",
    "@types/express-form-data": "^2.0.5",
    "advisory-lock": "^2.0.0",
    "async-retry": "^1.3.3",
    "axios": "^1.6.2",
    "axios-oauth-client": "^2.0.4",
    "axios-token-interceptor": "^0.2.0",
    "body-parser": "^2.2.1",
    "cron": "^4.3.0",
    "csv-parse": "^6.0.0",
    "dotenv": "^17.2.0",
    "easy-soap-request": "^5.5.0",
    "express": "^5.0.0",
    "express-form-data": "^3.0.0",
    "express-prom-bundle": "^8.0.0",
    "express-rate-limit": "^8.0.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "fast-safe-stringify": "^2.1.1",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.23",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "nconf": "^0.13.0",
    "nocache": "^4.0.0",
    "node-html-parser": "^7.0.0",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-openidconnect-keycloak-idp": "^0.1.6",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "reflect-metadata": "^0.2.1",
    "session-file-store": "^1.5.0",
    "uuid": "^11.0.0",
    "wait-for-expect": "^4.0.0",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^10.0.0",
    "@types/async-retry": "^1.4.8",
    "@types/axios-token-interceptor": "^0.2.8",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.23",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.0.0",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.0",
    "@types/session-file-store": "^1.2.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^11.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^17.0.0",
    "jest": "^29.7.0",
    "jest-badges": "^0.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^16.0.0",
    "mock-req-res": "^1.2.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^6.13.0",
    "prisma-mock": "^1.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --selectProjects unit",
    "test:cov": "npm run generate && jest --selectProjects unit --coverage --detectOpenHandles --forceExit",
    "test:integration": "jest --selectProjects integration",
    "build": "npm run generate && tsc && copyfiles -u 1 src/**/*.html dist",
    "generate": "prisma generate --schema=./src/v1/prisma/schema.prisma",
    "pull": "npx prisma db pull --schema=./src/v1/prisma/schema.prisma && prisma generate --schema=./src/v1/prisma/schema.prisma",
    "start": "node dist/src/src/server.js",
    "dev": "npm run generate && tsx watch src/server.ts",
    "lint:fix": "eslint --ext js,ts,tsx src --fix",
    "prettier:write": "prettier src --write"
  },
  "jest": {
    "projects": [
      {
        "displayName": "unit",
        "rootDir": "src",
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "testRegex": ".*\\.spec\\.(t|j)s$",
        "transformIgnorePatterns": [
          "node_modules/(?!@faker-js).+"
        ],
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
          "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "coveragePathIgnorePatterns": [
          "/node_modules/",
          "/test/"
        ],
        "testEnvironment": "node"
      },
      {
        "displayName": "integration",
        "rootDir": "tests/integration",
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "testRegex": ".*\\.spec\\.(t|j)s$",
        "transformIgnorePatterns": [
          "node_modules/(?!@faker-js).+"
        ],
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
          "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "coveragePathIgnorePatterns": [
          "/node_modules/",
          "/test/",
          "./src/server.ts"
        ],
        "testEnvironment": "node"
      }
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura",
      "jest-badges"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "prettier": {
    "singleQuote": true
  }
}
